{
    "name": "legalthings/legalevents",
    "version": "0.1.0",
    "description": "Event chain service",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-sodium": "2.*",
        "jasny/config" : "^1.1.2",
        "jasny/db-mongo" : "dev-master as 1.3.0",
        "jasny/controller": "^1.2",
        "jasny/php-functions" : "^3.0",
        "jasny/validation-result" : "^1.0.0",
        "jasny/error-handler": "^0.2.0",
        "jasny/social": "^0.4.3",
        "jasny/formbuilder": "~0.1.0",
        "jasny/assetic-extensions": "^1.0",
        "monolog/monolog": "1.*",
        "rollbar/rollbar": "^1.1.0",
        "mouf/picotainer": "^1.1",
        "oscarotero/psr7-middlewares": "^3.20",
        "symfony/yaml" : "^2.4.5",
        "guzzlehttp/guzzle": "^6.3",
        "legalthings/lto-api": "dev-php7",
        "jasny/router": "^1.1",
        "jasny/dotkey": "^1.0",
        "jasny/http-message": "^1.3",
        "legalthings/base58-compat": "^0.1.0"
    },
    "require-dev" : {
        "jasny/php-code-quality" : "^2.0.0",
        "codeception/c3": "^2.0",
        "codeception/codeception": "^2.2",
        "jasny/codeception-module": "dev-master",
        "mikey179/vfsStream": "~1.2"
    },
    "autoload": {
        "classmap": ["lib/", "models/", "controllers/"],
        "files": ["vendor/jasny/php-functions/global.php"]
    },
    "config": {
        "bin-dir": "bin"
    }
}
